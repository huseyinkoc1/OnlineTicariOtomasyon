@using BilisimOfisiTicariOtomasyon.Models.Sınıflar
@model List<SatisHareket>
@{
    ViewBag.Title = "Siparislerim";
    Layout = "~/Views/Shared/_CariLayout.cshtml";
}
<br />
<h2>Sipariş Geçmişim</h2>
<br />
<table class="table table-active">
    <tr>
        <th>Ürün Adı</th>
        <th>Fiyat</th>
        <th>Tarih</th>
    </tr>
    @foreach(var x in Model)
            {
                <tr>
                    <td>@x.Urunler.urunAd</td>
                    <td>@x.satisHareketToplamTutar</td>
                    <td>@Convert.ToDateTime(x.satisHareketTarih).ToString("d")</td>
                </tr>
            }
</table>
