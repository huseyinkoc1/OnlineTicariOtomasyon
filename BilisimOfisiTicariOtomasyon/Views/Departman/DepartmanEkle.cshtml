@model BilisimOfisiTicariOtomasyon.Models.Sınıflar.Departman
@{
    ViewBag.Title = "DepartmanEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<br />
<h2>Yeni Departman</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.departmanAd)
    @Html.TextBoxFor(x => x.departmanAd, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-info">Kayıt</button>
</form>